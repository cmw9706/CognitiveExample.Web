{
  "TwitterApiCollection": {
    "UserInformationEndpoint": "https://api.twitter.com/1.1/users/show.json",
    "SearchHandlesEndpoint": "https://api.twitter.com/1.1/search/tweets.json",
    "TokenEndpoint": "https://api.twitter.com/oauth2/token",
    "ConsumerApiKey": "{ CONSUMER_KEY }",
    "ConsumerApiSecret": "{ CONSUMER_SECRET }",
    "AuthenticationRequestBody": "grant_type=client_credentials",
    "PostContentType": "application/x-www-form-urlencoded;charset=UTF-8",
    "TweetGetCount": "100"
  },
  "CognitiveServicesConfiguration": {
    "TextAnalyticsApiConfig": {
      "AzureRegion": "Eastus2",
      "SubscriptionKey": "{ SUBSCRIPTION_KEY }"
    }
  }
}